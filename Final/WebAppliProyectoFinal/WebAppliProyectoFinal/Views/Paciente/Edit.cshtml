@{
}
<html>
<head>
</head>
<body>
    <div class="tabla">
    </div>
    <form asp-controller="Paciente" asp-action="editPaciente" method="post">
        <input type="hidden" asp-for="Idpa" />
        <br />
        Nombre:<input type="text" asp-for="Nompa" placeholder="Ingrese el nuevo Nombre del Paciente" required />
        <br />
        Apellido:<input type="text" asp-for="Apepa" placeholder="Ingrese el nuevo Apellido del Paciente" required />
        <br />
        Correo:<input type="text" asp-for="Corpa" placeholder="Ingrese el nuevo Correo del Paciente" required />
        <br />
        DNI:<input type="text" asp-for="Dnipa" placeholder="Ingresar el nuevo DNI de telefono del Paciente" required />
        <br />
        Celular:<input type="text" asp-for="Celpa" placeholder="Ingresar el nuevo Celular del Paciente" required />
        <br />
        <input type="submit" class="btn btn-success" value="Editar" />
        <a asp-action="Index" class="btn btn-primary m-2">Salir</a>
    </form>
</body>
</html>